<p id="notice"><%= notice %></p>

<h1>Type Projects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_projects.each do |type_project| %>
      <tr>
        <td><%= type_project.name %></td>
        <td><%= type_project.description %></td>
        <td><%= link_to 'Show', type_project %></td>
        <td><%= link_to 'Edit', edit_type_project_path(type_project) %></td>
        <td><%= link_to 'Destroy', type_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Project', new_type_project_path %>
